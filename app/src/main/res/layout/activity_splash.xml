<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SplashScreen">
    <ImageView
            android:layout_width="167dp"
            android:layout_height="161dp" app:srcCompat="@drawable/ic_launcher_background"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imgImagenMia_NOTENGOFOTO" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="150dp"/>
    <TextView
            android:text="@string/lblnombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/lblNombre"
            app:layout_constraintTop_toBottomOf="@+id/imgImagenMia_NOTENGOFOTO"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="100dp"/>
    <TextView
            android:text="@string/lblDNI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/lblDNI"
            app:layout_constraintTop_toBottomOf="@+id/lblNombre" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="50dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="100dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>